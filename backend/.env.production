# Production Environment Variables
# Copy this to .env and fill in your values

# Server
NODE_ENV=production
PORT=3001

# Solana Network
# For production, use mainnet-beta:
# RPC_ENDPOINT=https://api.mainnet-beta.solana.com
# Or use a dedicated RPC (recommended):
# RPC_ENDPOINT=https://solana-mainnet.g.alchemy.com/v2/YOUR_KEY
RPC_ENDPOINT=https://api.devnet.solana.com

# Program IDs (update when deployed to mainnet)
FACTORY_PROGRAM_ID=Factory11111111111111111111111111111111111
MANAGER_PROGRAM_ID=Manager11111111111111111111111111111111111

# Treasury Wallet (your production wallet)
TREASURY_WALLET=46hYFV39fxucRsidrE1qffXNXiZykX8yG9qNnmeia3ez

# Security: Encryption Master Key
# CRITICAL: Keep this secret! Use AWS Secrets Manager in production
# Generate new one with: openssl rand -hex 32
ENCRYPTION_MASTER_KEY=your_production_encryption_key_here

# Database (use PostgreSQL in production)
DATABASE_URL=postgresql://user:password@localhost:5432/agentfun_prod

# AWS KMS (optional, for enhanced security)
AWS_REGION=us-east-1
AWS_KMS_KEY_ID=your-kms-key-id

# Gemini AI API Key (for subagents)
GEMINI_API_KEY=your_gemini_api_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ORIGIN=https://your-domain.com

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=/var/log/agent-fun

# Monitoring (optional)
SENTRY_DSN=your_sentry_dsn_here
NEW_RELIC_LICENSE_KEY=your_newrelic_key_here

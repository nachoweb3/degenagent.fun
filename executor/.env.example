RPC_ENDPOINT=https://api.devnet.solana.com
GEMINI_API_KEY=your-gemini-api-key-here
BACKEND_API_URL=http://localhost:3001/api
EXECUTION_INTERVAL_MINUTES=5
MANAGER_PROGRAM_ID=Manager11111111111111111111111111111111111

# Security: Encryption Master Key (REQUIRED for secure key storage)
# MUST be the same as backend's ENCRYPTION_MASTER_KEY
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# CRITICAL: Keep this secret! Store in AWS Secrets Manager in production
ENCRYPTION_MASTER_KEY=GENERATE_YOUR_OWN_32_BYTE_HEX_KEY_HERE
